{
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
      "external_ui_download_detour": "手动选择",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": true
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "google",
        "type": "https",
        "server": "8.8.8.8",
        "detour": "手动选择"
      },
      {
        "tag": "local",
        "type": "https",
        "server": "223.5.5.5",
        "detour": "direct"
      },
      {
        "tag": "fakeip",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "rule_set": "geolocation-cn",
        "server": "local"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "server": "google",
        "strategy": "prefer_ipv4",
        "client_subnet": "114.114.114.114/24"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "fakeip",
        "rewrite_ttl": 1
      }
    ],
    "strategy": "ipv4_only",
    "independent_cache": true
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "address": [
        "172.19.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "auto_route": true,
      "strict_route": true
    },
    {
      "tag": "mixed-in",
      "type": "mixed",
      "listen": "::",
      "listen_port": 1080,
      "set_system_proxy": false
    }
  ],
  "outbounds": [
    {
      "tag": "手动选择",
      "type":"selector",
      "outbounds": ["{all}"]
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {,
    "auto_detect_interface": true,
    "default_domain_resolver": {
      "server": "local",
      "strategy": "prefer_ipv4"
    },
    "final": "手动选择",
    "rules": [
      {
        "action": "sniff",
        "timeout": "1s"
      },
      {
        "inbound": "tun-in",
        "action": "resolve"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": ["geolocation-cn"],
        "outbound": "direct"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "inbound": "mixed-in"
          },
          {
            "rule_set": "geolocation-!cn",
            "invert": true
          }
        ],
        "action": "resolve"
      },
      {
        "rule_set": ["geoip-cn"],
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geoip/cn.srs",
        "download_detour": "手动选择"
      },
      {
        "tag": "geolocation-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/geolocation-cn.srs",
        "download_detour": "手动选择"
      },
      {
        "tag": "geolocation-!cn",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/geolocation-!cn.srs",
        "download_detour": "手动选择"
      }
    ]
  }
}
